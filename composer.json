{
    "name": "galstval/gitlab-webhook-manager",
    "description": "A Slim framework based library to handle GitLab webhooks and execute git commands",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "slim/slim": "^4.0",
        "vlucas/phpdotenv": "^5.0",
        "symfony/process": "^6.0",
        "slim/psr7": "^1.0",
        "monolog/monolog": "^2.0"
    },
    "authors": [
        {
            "name": "Valiko Galstyan",
            "email": "valiko.galstyan@gmail.com"
        }
    ],
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Galstval\\GitLabWebhookManager\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
